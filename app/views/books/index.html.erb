<h1>Listing books</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @books.each do |book| %>
  <tr>
    <td><%= book.name %></td>
    <td><%= link_to 'Show', [book.student_library, book] %></td>
    <td><%= link_to 'Edit', edit_student_library_book_path(book.student_library, book) %></td>
    <td><%= link_to 'Destroy', [book.student_library, book], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Book', new_student_library_book_path(params[:student_library_id]) %>
